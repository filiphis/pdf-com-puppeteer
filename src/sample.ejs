<!doctype html>
<html lang="pt-br">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <style>
      @font-face {
        font-family: "SharonSans";
        font-weight: normal;
        font-style: normal;
        src: url(data:application/font-woff;charset=utf-8;base64,)
          format("woff");
        font-display: swap;
      }

      *,
      ::after,
      ::before {
        box-sizing: border-box;
        padding: 0;
        margin: 0;
        font-family: SharonSans, sans-serif;
        max-width: 360px;
      }

      html,
      body {
        height: auto;
        width: 360px;
        background-color: white;
      }

      body {
        display: flex;
        flex-direction: column;
      }

      .container {
        padding: 0 24px;
      }

      .text__description__regular--neutralHigh,
      .paragraph__regular--neutralHigh, .paragraph__regular--neutralHigh--bold {
        color: #303742;
        fill: #303742;
        font-family: SharonSans;
        font-weight: 400;
        font-size: 12px;
        max-width: 312px;
        line-height: 16.8px;
        letter-spacing: -0.01em;
      }

      .paragraph__regular--neutralHigh--bold {
        font-weight: bold;
      }

      .content--neutralHigh,
      .content--neutralHigh--bold {
        font-family: SharonSans;
        font-size: 12px;
        line-height: 140%;
        letter-spacing: -0.01em;
        color: #303742;
      }
      .content--neutralHigh--bold {
        font-weight: 700;
      }

      .paragraph__regular--neutralHigh {
        /* margin: 16px 0; */
      }

      .body__small__regular--white {
        font-family: SharonSans;
        color: #ffffff;
        fill: #ffffff;
        font-size: 13px;
        font-weight: 400;
        line-height: 18.85px;
        letter-spacing: -0.01em;
        max-width: 312px;
      }

      .heading__large__bold--white {
        font-family: SharonSans;
        color: #ffffff;
        fill: #ffffff;
        font-size: 24px;
        font-weight: bold;
        line-height: 28.8px;
        letter-spacing: -0.01em;
        max-width: 312px;
      }

      .heading__medium__bold--neutralHigh {
        font-family: SharonSans;
        color: #303742;
        fill: #303742;
        font-size: 16px;
        font-weight: 700;
        line-height: 21.75px;
        letter-spacing: -0.01em;
        text-align: left;
        margin: 16px 0;
      }

      .body__small__regular,
      .body__small__regular--white {
        font-family: SharonSans;
        font-size: 13px;
        font-weight: 400;
        line-height: 18.85px;
        letter-spacing: -0.01em;
        max-width: 312px;
      }

      .body__small__regular--white {
        color: #ffffff;
        fill: #ffffff;
      }

      .heading__large__bold,
      .heading__large__bold--white {
        font-family: SharonSans;
        font-size: 24px;
        font-weight: 700;
        line-height: 28.8px;
        letter-spacing: -0.01em;
        max-width: 312px;
      }
      .heading__large__bold--white {
        color: #ffffff;
        fill: #ffffff;
      }

      .heading__medium__bold,
      .heading__medium__bold--neutralHigh,
      .heading__medium__bold--white {
        font-family: SharonSans;
        font-size: 16px;
        font-weight: 700;
        line-height: 21.75px;
        letter-spacing: -0.01em;
        text-align: left;
      }

      .heading__medium__bold--neutralHigh {
        color: #303742;
        fill: #303742;
      }

      .heading__medium__bold--white {
        fill: #ffffff;
        color: #ffffff;
      }

      .bold {
        font-weight: 700;
      }

      .mt-md {
        margin-top: 16px;
      }
      .mtb-md {
        margin-top: 16px;
        margin-bottom: 16px;
      }
      .m-md {
        margin: 16px;
      }

      .p-xl {
        padding: 24px;
      }

      section {
        display: flex;
        flex-direction: column;
        gap: 16px;
        margin-top: 16px;
      }

      .header {
        display: flex;
        flex-direction: column;
        background-color: #008e5a;
        border-bottom-left-radius: 32px;
      }

      .header__stoneContent {
        background-color: #00a868;
        padding: 24px;
        display: flex;
        flex-direction: column;
        border-bottom-right-radius: 32px;
      }

      .header__logoAndDate {
        display: flex;
        align-items: center;
        justify-content: space-between;
      }

      .header__clientContent {
        display: flex;
        flex-direction: column;
        gap: 8px;
        padding: 24px;
      }

      table {
        background-color: #f7f7fa;
        padding: 16px;
        border-radius: 8px;
        width: 312px;
        display: flex;
        flex-direction: column;
        gap: 8px;
        text-align: left;
      }

      tbody {
        display: flex;
        flex-direction: column;
        gap: 8px;
      }

      tr {
        display: flex;
        position: relative;
        align-items: center;
      }

      tbody > tr:not(:first-child) {
        border-top: 1px solid #30374233;
        padding-top: 8px;
      }
      tr > * {
        flex: 1;
      }

      tr > td:first-child {
        display: flex;
        align-items: center;
        justify-content: left;
      }

      .table__sumFees__span {
        display: inline-block;
        max-width: 42px;
      }

      .table__sumFees tr > td:first-child, .table__sumFees tr > th:first-child {
        flex-grow: 2;
        justify-content: left;
        gap: 4px;
        max-width: 100px;
      }

      .anticipationFeeSection {
        display: flex;
        gap: 12px;
      }

      .anticipationFeeSection__block {
        width: 150px;
        height: 88px;
        display: flex;
        flex-direction: column;
        gap: 8px;
        justify-content: center;
        align-items: center;
        background-color: #f7f7fa;
        border-radius: 8px;
        border: 1px solid #30374233;
        position: relative;
      }

      .anticipationFeeSection__block--active {
        border: 1px solid #00a868;
        position: relative;
      }

       .anticipationFeeSection__block--active::before {
        content: "ATIVA";
        position: absolute;
        display: flex;
        align-items: center;
        justify-content: center;
        line-height: 0;
        width: 51px;
        height: 19px;
        bottom: -10px;
        padding: 3px 8px 3px 8px;
        gap: 8px;
        border-radius: 21px;
        background-color: #00a868;
        color: #ffffff;
        font-size: 11px;
        font-weight: 600;
        line-height: 13.2px;
        letter-spacing: 0.04em;
      }

      .anticipationFeeSection__block--activeDiv {
        display: inline-block;
        position: relative;
        bottom: -10px;
        width: 51px;
        height: 19px;
        padding: 3px 8px 3px 8px;
        gap: 8px;
        border-radius: 21px;
        background-color: #00a868;
        color: #ffffff;
        font-size: 11px;
        font-weight: 600;
        line-height: 13.2px;
        letter-spacing: 0.04em;
      }

      .anticipationFeeSection__text {
        max-width: 78px;
        text-align: center;
      }

      .divBlock {
        width: 100%;
        max-width: 312px;
        background-color: #F7F7FA;
        border: 1px solid #30374233;
        border-radius: 8px;
        padding: 16px;
        display: flex;
        flex-direction: column;
        justify-content: center ;
        align-items: center;
        gap: 8px;
        text-align: center;
      }

      .divBlockWarning {
        display: flex;
        flex-direction: column;
        gap: 8px;
        padding: 16px;
        text-align: left;
        background-color: #f384281a;
        border: 1px solid #f38428;
        border-radius: 8px;
      }

      .machinesQuantityAndValueContainer {
        display: flex;
        gap: 12px;

      }

      div.activeTag {
        border: 1px solid #00a868;
      }
    </style>
  </head>

